description: {{ description | indent(2, false) | replace(":", "-") }}
enabled: true
entry_point: run.py
name: {{ method }}
parameters:
  end_point:
    default: {{ method }}
    immutable: true
    type: string{% for name, default, required in parameters %}
  {{ name }}:
    required: {{ required }}{% if default != "" %}
    default: {{ default }}{% endif %}
    type: string{% endfor %}
runner_type: python-script
